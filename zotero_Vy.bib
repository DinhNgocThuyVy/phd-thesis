  author = {Liu, Yue and Zheng, Mei and Yu, Mingyuan and Cai, Xuhui and Du, Huiyun and Li, Jie and Zhou, Tian and Yan, Caiqing and Wang, Xuesong and Shi, Zongbo and Harrison, Roy M. and Zhang, Qiang and He, Kebin},
