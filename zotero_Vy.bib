@article{Chang2018,
  title = {First Long-Term and near Real-Time Measurement of Trace Elements in {{China}}'s Urban Atmosphere: Temporal Variability, Source Apportionment and Precipitation Effect},
  shorttitle = {First Long-Term and near Real-Time Measurement of Trace Elements in {{China}}'s Urban Atmosphere},
  author = {Chang, Yunhua and Huang, Kan and Xie, Mingjie and Deng, Congrui and Zou, Zhong and Liu, Shoudong and Zhang, Yanlin},
