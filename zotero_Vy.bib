  file = {C:\Users\dinhngot\Documents\20.PhD\Manuscript\Chapter 1-Litterature review\Reference\Modelling\Pekel_2025_Towards source specific oxidative potential exposure modelling_Intercomparison of modelled LOTOS-EUROS with observational based PM10.pdf}
