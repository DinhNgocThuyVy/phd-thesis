  author = {Rosofsky, Anna S. and Fabian, M. Patricia and Ettinger De Cuba, Stephanie and Sandel, Megan and Coleman, Sharon and Levy, Jonathan I. and Coull, Brent A. and Hart, Jaime E. and Zanobetti, Antonella},
