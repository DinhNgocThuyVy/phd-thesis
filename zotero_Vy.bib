  shorttitle = {Characteristics and {{Oxidative Potential}} of {{Ambient PM2}}.5 in the {{Yangtze River Delta Region}}},
  author = {Cui, Yaojia and Zhu, Longwei and Wang, Hui and Zhao, Zhuzi and Ma, Shuaishuai and Ye, Zhaolian},
