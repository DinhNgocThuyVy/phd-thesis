@article{Li2022,
  title = {Association of {{PM2}}.5 and {{PM10}} with {{Acute Exacerbation}} of {{Chronic Obstructive Pulmonary Disease}} at Lag0 to Lag7: {{A Systematic Review}} and {{Meta-Analysis}}},
