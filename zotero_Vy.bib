@article{Hernandez-Pellon2019,
  title = {Using Multi-Site Data to Apportion {{PM-bound}} Metal(Loid)s: {{Impact}} of a Manganese Alloy Plant in an Urban Area},
  author = {Hernández-Pellón, A. and Fernández-Olmo, I.},
  date = {2019-02-15},
