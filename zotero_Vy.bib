@article{Via2022,
  title = {\emph{Rolling} vs. \emph{Seasonal} {{PMF}}: Real-World Multi-Site and Synthetic Dataset Comparison},
  shorttitle = {\emph{Rolling} vs. \emph{Seasonal} {{PMF}}},
  author = {Via, Marta and Chen, Gang and Canonaco, Francesco and Daellenbach, Kaspar R. and Chazeau, Benjamin and Chebaicheb, Hasna and Jiang, Jianhui and Keernik, Hannes and Lin, Chunshui and Marchand, Nicolas and Marin, Cristina and O'Dowd, Colin and Ovadnevaite, Jurgita and Petit, Jean-Eudes and Pikridas, Michael and Riffault, Véronique and Sciare, Jean and Slowik, Jay G. and Simon, Leïla and Vasilescu, Jeni and Zhang, Yunjiang and Favez, Olivier and Prévôt, André S. H. and Alastuey, Andrés and Cruz Minguillón, María},
  date = {2022-09-27},
  journaltitle = {Atmos. Meas. Tech.},
  volume = {15},
