@article{Weichenthal2019,
  title = {Within-{{City Spatial Variations}} in {{Multiple Measures}} of {{PM}}{\textsubscript{2.5}} {{Oxidative Potential}} in {{Toronto}}, {{Canada}}},
  author = {Weichenthal, Scott and Shekarrizfard, Maryam and Traub, Alison and Kulka, Ryan and Al-Rijleh, Kenan and Anowar, Sabreena and Evans, Greg and Hatzopoulou, Marianne},
  date = {2019-03-05},
  journaltitle = {Environ. Sci. Technol.},
  volume = {53},
