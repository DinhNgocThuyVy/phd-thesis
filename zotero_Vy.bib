@article{Daellenbach2024,
  title = {Substantial Contribution of Transported Emissions to Organic Aerosol in {{Beijing}}},
  author = {Daellenbach, Kaspar R. and Cai, Jing and Hakala, Simo and Dada, Lubna and Yan, Chao and Du, Wei and Yao, Lei and Zheng, Feixue and Ma, Jialiang and Ungeheuer, Florian and Vogel, Alexander L. and Stolzenburg, Dominik and Hao, Yufang and Liu, Yongchun and Bianchi, Federico and Uzu, Gaëlle and Jaffrezo, Jean-Luc and Worsnop, Douglas R. and Donahue, Neil M. and Kulmala, Markku},
  date = {2024-08},
  journaltitle = {Nat. Geosci.},
