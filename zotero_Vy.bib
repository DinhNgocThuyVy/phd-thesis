  author = {Daellenbach, Kaspar R. and Kourtchev, Ivan and Vogel, Alexander L. and Bruns, Emily A. and Jiang, Jianhui and Petäjä, Tuukka and Jaffrezo, Jean-Luc and Aksoyoglu, Sebnem and Kalberer, Markus and Baltensperger, Urs and El Haddad, Imad and Prévôt, André S. H.},
