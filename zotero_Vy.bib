@article{Lei2021,
  title = {Spatiotemporal Variations and Source Apportionment of Metals in Atmospheric Particulate Matter in {{Beijing}} and Its Surrounding Areas},
  author = {Lei, Wenkai and Zhang, Lan and Xu, Jing and Liu, Zirui and Xin, Jinyuan and Li, Xingru and Zhao, Wenji},
  date = {2021-11},
  journaltitle = {Atmospheric Pollution Research},
