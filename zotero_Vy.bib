}

@article{Caserini2017,
  title = {Influence of Climate Change on the Frequency of Daytime Temperature Inversions and Stagnation Events in the {{Po Valley}}: Historical Trend and Future Projections},
  author = {Caserini, Stefano and Giani, Paolo and Cacciamani, Carlo and Ozgen, Senem and Lonati, Giovanni},
