  author = {Su, Cui-Ping and Peng, Xing and Huang, Xiao-Feng and Zeng, Li-Wu and Cao, Li-Ming and Tang, Meng-Xue and Chen, Yao and Zhu, Bo and Wang, Yishi and He, Ling-Yan},
  date = {2020-10-12},
  journaltitle = {Atmos. Meas. Tech.},
