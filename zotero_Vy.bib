@article{Liu2018,
  title = {Metals and Oxidative Potential in Urban Particulate Matter Influence Systemic Inflammatory and Neural Biomarkers: {{A}} Controlled Exposure Study},
  shorttitle = {Metals and Oxidative Potential in Urban Particulate Matter Influence Systemic Inflammatory and Neural Biomarkers},
  author = {Liu, Ling and Urch, Bruce and Szyszkowicz, Mieczyslaw and Evans, Greg and Speck, Mary and Van Huang, Angela and Leingartner, Karen and Shutt, Robin H. and Pelletier, Guillaume and Gold, Diane R. and Brook, Jeffrey R. and Godri Pollitt, Krystal and Silverman, Frances S.},
