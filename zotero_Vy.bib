@article{Pietrogrande2019,
  title = {Review of {{PM Oxidative Potential Measured}} with {{Acellular Assays}} in {{Urban}} and {{Rural Sites}} across {{Italy}}},
  author = {Pietrogrande, Maria Chiara and Russo, Mara and Zagatti, Elisa},
  date = {2019-10-16},
  journaltitle = {Atmosphere},
