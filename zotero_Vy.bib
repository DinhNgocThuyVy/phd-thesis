@article{Pandolfi2020a,
  title = {Long-Range and Local Air Pollution: {{What}} Can We Learn from Chemical Speciation of Particulate Matter at Paired Sites?},
  author = {Pandolfi, Marco and Mooibroek, Dennis and Hopke, Philip and Van Pinxteren, Dominik and Querol, Xavier and Herrmann, Hartmut and Alastuey, Andrés and Favez, Olivier and Hüglin, Christoph and Perdrix, Esperanza and Riffault, Véronique and Sauvage, Stéphane and Van Der Swaluw, Eric and Tarasova, Oksana and Colette, Augustin},
