@article{Yang2016,
  title = {Children's Respiratory Health and Oxidative Potential of {{PM}}{\textsubscript{2.5}} : The {{PIAMA}} Birth Cohort Study},
  shorttitle = {Children's Respiratory Health and Oxidative Potential of {{PM}}{\textsubscript{2.5}}},
