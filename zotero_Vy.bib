  author = {Gugamsetty, Balakrishnaiah and Wei, Han and Liu, Chun-Nan and Awasthi, Amit and Hsu, Shih-Chieh and Tsai, Chuen-Jinn and Roam, Gwo-Dong and Wu, Yue-Chuen and Chen, Chung-Fang},
