@article{Caporale2021,
  title = {Particulate Matter 10 ({{PM10}}): Persistence and Trends in Eight {{European}} Capitals},
  author = {Caporale, Guglielmo Maria and Gil-Alana, Luis A. and Carmona-González, Nieves},
  date = {2021},
  journaltitle = {Air Qual. Atmosphere Health},
  volume = {14},
  number = {7},
  pages = {1097--1102},
  issn = {18739326},
  doi = {10.1007/s11869-021-01002-0},
  abstract = {This paper examines the statistical properties of daily PM10 in eight European capitals (Amsterdam, Berlin, Brussels, Helsinki, London, Luxembourg, Madrid and Paris) over the period 2014–2020 by applying a fractional integration framework; this is more general than the standard approach based on the classical dichotomy between I(0) stationary and I(1) non-stationary series used in most other studies on air pollutants. All series are found to be characterised by long memory and fractional integration, with orders of integration in the range (0, 1), which implies that mean reversion occurs and shocks do not have permanent effects. Persistence is the highest in the case of Brussels, Amsterdam and London. The presence of negative trends in Brussels, Paris and Berlin indicates some degree of success in reducing pollution in these capitals.},
  keywords = {Air pollutants,Fractional integration,Long memory,Persistence,PM10,Trends}
}

@article{Carbone2010,
