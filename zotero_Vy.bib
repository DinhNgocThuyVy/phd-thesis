@article{Chen2019,
  title = {The Effects of Fine and Coarse Particulate Matter on Lung Function among the Elderly},
  author = {Chen, Chi-Hsien and Wu, Chih-Da and Chiang, Hung-Che and Chu, Dachen and Lee, Kang-Yun and Lin, Wen-Yi and Yeh, Jih-I and Tsai, Kun-Wei and Guo, Yue-Liang Leon},
