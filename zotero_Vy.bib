@article{Zong2018,
  title = {{{PMF}} and {{PSCF}} Based Source Apportionment of {{PM2}}.5 at a Regional Background Site in {{North China}}},
  author = {Zong, Zheng and Wang, Xiaoping and Tian, Chongguo and Chen, Yingjun and Fu, Shanfei and Qu, Lin and Ji, Ling and Li, Jun and Zhang, Gan},
  date = {2018-05},
  journaltitle = {Atmospheric Research},
  volume = {203},
