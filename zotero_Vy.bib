  author = {Künzli, Nino and Mudway, Ian S. and Götschi, Thomas and Shi, Tingming and Kelly, Frank J. and Cook, Sarah and Burney, Peter and Forsberg, Bertil and Gauderman, James W. and Hazenkamp, Marianne E. and Heinrich, Joachim and Jarvis, Deborah and Norbäck, Dan and Payo-Losa, Felix and Poli, Albino and Sunyer, Jordi and Borm, Paul J.A.},
  date = {2006-05},
  journaltitle = {Environ Health Perspect},
