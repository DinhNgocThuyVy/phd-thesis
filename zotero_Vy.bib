@article{Gianini2012,
  title = {Comparative Source Apportionment of {{PM10}} in {{Switzerland}} for 2008/2009 and 1998/1999 by {{Positive Matrix Factorisation}}},
  author = {Gianini, M. F.D. and Fischer, A. and Gehrig, R. and Ulrich, A. and Wichser, A. and Piot, C. and Besombes, J. L. and Hueglin, C.},
