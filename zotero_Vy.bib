@article{Weber2018,
  title = {An Apportionment Method for the Oxidative Potential of Atmospheric Particulate Matter Sources: {{Application}} to a One-Year Study in {{Chamonix}}, {{France}}},
  author = {Weber, Samuël and Uzu, Gaëlle and Calas, Aude and Chevrier, Florie and Besombes, Jean Luc and Charron, Aurélie and Salameh, Dalia and Ježek, Irena and Moĉnik, Griŝa and Jaffrezo, Jean Luc},
  date = {2018-07},
