@article{Johnson2015,
  title = {Weighted {{Least Squares Estimates}} of {{Return Predictability Regressions}}},
  author = {Johnson, Travis L.},
  date = {2015},
  journaltitle = {SSRN Electron. J.},
  doi = {10.2139/ssrn.2561112},
  abstract = {Time varying volatility causes substantial heteroskedasticity in return predictability regressions. While OLS estimates and heteroskedasticity-consistent standard errors are asymptotically unbiased, they are inefficient compared to least squares estimates weighted by ex-ante return variance (WLS-EV). In small sample simulations, I show that using WLS-EV instead of OLS results in large efficiency gains, fewer false negatives, and avoids the bias associated with ex-post weighting schemes. Using WLS-EV also improves the out-of-sample performance of traditional predictors such as the dividend-to-price ratio. Finally, I show the predictability afforded by proxies for the variance risk premium is not robust to WLS-EV.}
}

