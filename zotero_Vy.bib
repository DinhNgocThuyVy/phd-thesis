  author = {Manousakas, M. and Diapouli, E. and Papaefthymiou, H. and Migliori, A. and Karydas, A.G. and Padilla-Alvarez, R. and Bogovac, M. and Kaiser, R.B. and Jaksic, M. and Bogdanovic-Radovic, I. and Eleftheriadis, K.},
