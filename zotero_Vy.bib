@article{He2021,
  title = {Personal {{Exposure}} to {{PM}}{\textsubscript{2.5}} {{Oxidative Potential}} in {{Association}} with {{Pulmonary Pathophysiologic Outcomes}} in {{Children}} with {{Asthma}}},
  author = {He, Linchen and Norris, Christina and Cui, Xiaoxing and Li, Zhen and Barkjohn, Karoline K. and Brehmer, Collin and Teng, Yanbo and Fang, Lin and Lin, Lili and Wang, Qian and Zhou, Xiaojian and Hong, Jianguo and Li, Feng and Zhang, Yinping and Schauer, James J. and Black, Marilyn and Bergin, Michael H. and Zhang, Junfeng Jim},
  date = {2021-03-02},
  journaltitle = {Environ. Sci. Technol.},
  volume = {55},
