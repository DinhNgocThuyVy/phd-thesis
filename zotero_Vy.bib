@article{Lavigne2018,
  title = {Fine {{Particulate Air Pollution}} and {{Adverse Birth Outcomes}}: {{Effect Modification}} by {{Regional Nonvolatile Oxidative Potential}}},
  shorttitle = {Fine {{Particulate Air Pollution}} and {{Adverse Birth Outcomes}}},
