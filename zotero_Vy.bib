  author = {Slama, Rémy and Thiebaugeorges, Olivier and Goua, Valérie and Aussel, Lucette and Sacco, Paolo and Bohet, Aline and Forhan, Anne and Ducot, Béatrice and Annesi-Maesano, Isabella and Heinrich, Joachim and Magnin, Guillaume and Schweitzer, Michel and Kaminski, Monique and Charles, Marie-Aline and {EDEN Mother–Child Cohort Study Group}},
  date = {2009-08},
  journaltitle = {Environ Health Perspect},
  volume = {117},
