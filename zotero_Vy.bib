@article{Khan2016,
  title = {Fine Particulate Matter in the Tropical Environment: Monsoonal Effects, Source Apportionment, and Health Risk Assessment},
  shorttitle = {Fine Particulate Matter in the Tropical Environment},
  author = {Khan, M. F. and Latif, M. T. and Saw, W. H. and Amil, N. and Nadzir, M. S. M. and Sahani, M. and Tahir, N. M. and Chung, J. X.},
  date = {2016-01-19},
  journaltitle = {Atmos. Chem. Phys.},
