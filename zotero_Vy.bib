@article{Giugliano2005,
  title = {Fine Particulate ({{PM2}}.5–{{PM1}}) at Urban Sites with Different Traffic Exposure},
  author = {Giugliano, Michele and Lonati, Giovanni and Butelli, Paola and Romele, Laura and Tardivo, Ruggero and Grosso, Mario},
