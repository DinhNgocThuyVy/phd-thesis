  author = {Zhang, Zilong and Weichenthal, Scott and Kwong, Jeffrey C. and Burnett, Richard T. and Hatzopoulou, Marianne and Jerrett, Michael and Van Donkelaar, Aaron and Bai, Li and Martin, Randall V. and Copes, Ray and Lu, Hong and Lakey, Pascale and Shiraiwa, Manabu and Chen, Hong},
