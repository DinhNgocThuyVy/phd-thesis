  author = {Bates, Josephine T. and Weber, Rodney J. and Abrams, Joseph and Verma, Vishal and Fang, Ting and Klein, Mitchel and Strickland, Matthew J. and Sarnat, Stefanie Ebelt and Chang, Howard H. and Mulholland, James A. and Tolbert, Paige E. and Russell, Armistead G.},
  date = {2015-10},
  journaltitle = {Environ. Sci. Technol.},
