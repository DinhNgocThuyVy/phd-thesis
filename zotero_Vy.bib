@article{Kawamura2016,
  title = {A Review of Dicarboxylic Acids and Related Compounds in Atmospheric Aerosols: {{Molecular}} Distributions, Sources and Transformation},
  author = {Kawamura, Kimitaka and Bikkina, Srinivas},
  date = {2016-03-15},
  journaltitle = {Atmospheric Res.},
  volume = {170},
