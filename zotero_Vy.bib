@article{Sellier2014,
  title = {Health Effects of Ambient Air Pollution: {{Do}} Different Methods for Estimating Exposure Lead to Different Results?},
  shorttitle = {Health Effects of Ambient Air Pollution},
  author = {Sellier, Yann and Galineau, Julien and Hulin, Agnes and Caini, Fabrice and Marquis, Nathalie and Navel, Vladislav and Bottagisi, Sebastien and Giorgis-Allemand, Lise and Jacquier, Claire and Slama, Remy and Lepeule, Johanna},
  date = {2014-05},
  journaltitle = {Environment International},
  volume = {66},
