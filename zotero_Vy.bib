@article{Yang2020,
  title = {Organ-on-a-{{Chip}}: {{Opportunities}} for {{Assessing}} the {{Toxicity}} of {{Particulate Matter}}},
  shorttitle = {Organ-on-a-{{Chip}}},
  author = {Yang, Jia-Wei and Shen, Yu-Chih and Lin, Ko-Chih and Cheng, Sheng-Jen and Chen, Shiue-Luen and Chen, Chong-You and Kumar, Priyank V. and Lin, Shien-Fong and Lu, Huai-En and Chen, Guan-Yu},
