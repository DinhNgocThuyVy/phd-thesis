@article{Betsou2021,
  title = {First-{{Time Source Apportionment Analysis}} of {{Deposited Particulate Matter}} from a {{Moss Biomonitoring Study}} in {{Northern Greece}}},
  author = {Betsou, Chrysoula and Diapouli, Evangelia and Tsakiri, Evdoxia and Papadopoulou, Lambrini and Frontasyeva, Marina and Eleftheriadis, Konstantinos and Ioannidou, Alexandra},
