@article{Liang2021,
  title = {Source Apportionment of Marine Atmospheric Aerosols in Northern {{South China Sea}} during Summertime 2018},
  author = {Liang, Baoling and Cai, Mingfu and Sun, Qibin and Zhou, Shengzhen and Zhao, Jun},
  date = {2021-11},
